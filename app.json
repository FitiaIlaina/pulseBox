{
  "expo": {
    "name": "camera",
    "slug": "pulse-box",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pulseBox.png",
    "scheme": "camera",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "assets": [
      "assets/data/*.mp3"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pulseBox.png",
        "backgroundColor": "#000"
      },
      "package": "com.anonymous.camera",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/pulseBox.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/pulseBox.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3d7fab86-d3ef-4597-94f3-c555ae13460e"
      }
    },
    "owner": "ilainafitia"
  }
}
